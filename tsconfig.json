{
    "compilerOptions": {
        "baseUrl": ".",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "outDir": "app/client-js",
        "paths": { },
        "removeComments": false,
        "sourceMap": true,
        "strict": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "esnext"
    },
    "include": [
        "app", "app/client/.env",
    ],
    "exclude": [
        "node_modules",
        "umi",
    ]
}
